@charset "utf-8";
@media (min-width: 805px) {
  .pc-header-nav {
    display: block;
  }
}
.navbar-default {
  background-color: #390C20;
  border-color: #390C20;
}
.navbar {
  margin-bottom: 0;
}
@media (max-width: 1500px) and (min-width: 805px) {
  .pc-nav-container {
    padding-right: 15px;
    padding-left: 15px;
  }
}
@media (min-width: 1200px) {
  .container {
    width: 1200px;
  }
}
.navbar-header {
  float: left;
}
.navbar-header .nav-logo {
  float: left;
  display: inline-block;
  margin-top: 5px;
  margin-left: 10px;
  margin-right: 10px;
}
.navbar-header .nav-logo img {
  height: 40px;
  width: 40px;
}
img {
  vertical-align: middle;
}
.middle-nav-ul {
  text-align: center;
  list-style: none;
  margin: 0px;
  height: 50px;
}
@media (min-width: 768px) {
  .navbar-nav > li {
    float: left;
  }
  .nav > li {
    position: relative;
    display: block;
  }
  .v2_locale_icon {
    margin-right: 10px;
  }
}
.navbar-default .navbar-nav > li > a.pc-locale-icon {
  color: #fff;
}
@media (min-width: 768px) {
  .navbar-nav > li > a {
    line-height: 20px;
    padding-left: 0;
    padding-right: 0;
  }
  .nav > li > a {
    position: relative;
    display: block;
  }
}
.navbar-nav > li a.dropdown-toggle {
  color: #fff;
}
.fa {
  display: inline-block;
  font: normal normal normal 14px/1 FontAwesome;
  font-size: inherit;
  text-rendering: auto;
  -webkit-font-smoothing: antialiased;
  color: #fff;
}
a:link {
  text-decoration: none;
  cursor: pointer;
}
.navbar-default .navbar-nav > li > a:hover,
.navbar-default .navbar-nav > li > a:focus {
  color: #fff;
  background-color: transparent;
}
.navbar-nav > li > .dropdown-menu {
  margin-top: 0;
  border-top-right-radius: 0;
  border-top-left-radius: 0;
}
.open > .dropdown-menu {
  display: block;
}
.dropdown-menu {
  position: absolute;
  top: 100%;
  left: 0;
  z-index: 1000;
  display: none;
  float: left;
  min-width: 160px;
  padding: 5px 0;
  margin: 2px 0 0;
  list-style: none;
  font-size: 14px;
  text-align: left;
  background-color: white;
  border: 1px solid rgba(0, 0, 0, 0.15);
  border-radius: 4px;
  box-shadow: 0 6px 12px rgba(0, 0, 0, 0.175);
  background-clip: padding-box;
}
.middle-nav-ul-li {
  color: #fff;
  padding-top: 15px;
  margin: 0 10px;
}
.middle-nav-ul .navbar-font-color {
  padding-left: 0;
  padding-right: 0;
}
.navbar-default .navbar-nav > li > a {
  color: #fff;
}
.navbar-default .navbar-nav .open .dropdown-menu > li > a {
  color: #390C20;
}
@media (min-width: 768px) {
  .container > .navbar-header,
  .container > .navbar-collapse,
  .container-fluid > .navbar-header,
  .container-fluid > .navbar-collapse {
    margin-right: 0;
    margin-left: 0;
  }
}
@media (min-width: 768px) {
  .navbar-header {
    margin-right: -3%;
  }
  .navbar-header {
    margin-right: 0px !important;
  }
}
@media (min-width: 768px) {
  .navbar-header {
    float: left;
  }
}
.navbar-header .nav-search-dom {
  display: inline-block;
  float: left;
  position: relative;
}
@media (max-width: 1500px) and (min-width: 805px) {
  .navbar-header .nav-search-dom {
    width: 100% !important;
  }
}
.navbar-header .nav-search-dom .form-control {
  border-radius: 20px;
  height: 30px;
  margin-top: 10px;
}
.form-control {
  display: block;
  width: 100%;
  padding: 6px 12px;
  font-size: 14px;
  line-height: 1.42857143;
  color: #555555;
  background-color: white;
  background-image: none;
  border: 1px solid #cccccc;
  box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075);
}
.navbar-header .nav-search-dom .search-icon {
  position: absolute;
  right: 15px;
  top: 40%;
  font-size: 14pt;
  color: #390c20;
  cursor: pointer;
}
